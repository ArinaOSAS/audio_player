<Application x:Class="Denzi.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Denzi"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/English.xaml"/>
                <ResourceDictionary Source="Resource/Russian.xaml"/>
                <ResourceDictionary Source="Resource/Black.xaml"/>
                <ResourceDictionary Source="Resource/Write.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
